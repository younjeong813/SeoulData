{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "sourceMap": true,
    "lib": ["DOM", "ES2015", "DOM.Iterable"]
  },
  "include": ["*.js"],
  "exclude": ["node_modules"]
}
